# Database connection settings
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/sticky_notes
SPRING_DATASOURCE_USERNAME=your_username
SPRING_DATASOURCE_PASSWORD=your_password

# Cloud SQL settings (for production)
SPRING_CLOUD_GCP_SQL_ENABLED=false
SPRING_CLOUD_GCP_SQL_DATABASE_NAME=sticky_notes
SPRING_CLOUD_GCP_SQL_INSTANCE_CONNECTION_NAME=your-project:your-region:your-instance

# Application profile
SPRING_PROFILES_ACTIVE=local

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-that-is-at-least-256-bits-long

# Port for Cloud Run (will override server.port in application.properties)
PORT=8081
